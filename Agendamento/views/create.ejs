<body>
    <h1>Cadastro de consulta</h1>
    <hr>
    <form action="create" method="POST">
        <input type="text" name="name" id="name" placeholder="Nome do paciente" class="form-control">
        <input type="email" name="email" id="email" placeholder="email@email.com" class="form-control">
        <input type="text" name="cpf" id="cpf" placeholder="CPF" class="form-control cpf">
        <input type="text" name="description" id="description" placeholder="DescriÃ§Ã£o" class="form-control">
        <input type="date" name="date" id="date" class="form-control">
        <input type="time" name="time" id="time" class="form-control">
        <button type="submit" class="btn btn-success w-100">Agendar</button>
    </form>
</body>
